<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no" />
    
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css" integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous" />
    <link rel="stylesheet" href="{{ siteUrl }}assets/forms/css/main.css">
    <link rel="stylesheet" href="{{ siteUrl }}/assets/forms/css/docs.css">
</head>

<body role="document">
{% if page is not defined %}
    {% set page = "Home" %}
{% endif %}

<nav class="navbar fixed-top navbar-expand-lg navbar-dark bg-dark">
    <a class="navbar-brand" href="{{ siteUrl }}">
        <!--img src="{{ siteUrl }}assets/forms/images/icon.svg" alt="Express Forms Demo"-->
    <span style="color:darkorange">Jay</span>Greentree
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
    </button>

    <div class="collapse navbar-collapse" id="navbarSupportedContent">
        <ul class="navbar-nav mr-auto">
            <li class="nav-item {{ page == "" ? "active" }}">
                <a class="nav-link" href="{{ siteUrl }}">Home</a>
            </li>
            <li class="nav-item {{ page == "manual-macro" ? "active" }}">
                <a class="nav-link" href="{{ siteUrl }}about">About</a>
            </li>
            <li class="nav-item {{ page == "automated" ? "active" }}">
                <a class="nav-link" href="{{ siteUrl }}"></a>
            </li>
            <li class="nav-item {{ page == "ajax" ? "active" }}">
                <a class="nav-link" href="{{ siteUrl }}"></a>
            </li>
            <li class="nav-item {{ page == "recaptcha" ? "active" }}">
                <a class="nav-link" href="{{ siteUrl }}"></a>
            </li>
        </ul>
        <div class="navbar-nav dropdown">
            <a class="nav-link dropdown-toggle" href="#" id="navbarDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Resources</a>
            <div class="dropdown-menu" aria-labelledby="navbarDropdown">
                <a class="dropdown-item" href="https://plugins.craftcms.com/express-forms/">Plugin Store</a>
                <a class="dropdown-item" href="https://docs.solspace.com/craft/express-forms/v2/">Documentation</a>
                <a class="dropdown-item" href="https://docs.solspace.com/craft/express-forms/v2/setup/troubleshooting.html">Troubleshooting</a>
                <a class="dropdown-item" href="https://docs.solspace.com/craft/express-forms/v2/support.html">Support</a>
            </div>
        </div>
    </div>
</nav>

<div class="container">

{% block content %}

{% endblock %}
</div>

{% block footer %}
    <div class="navbar footer">
        <p class="text-muted">&copy; {{ "now"|date('Y') }} {{siteName}}.</p>
    </div>
{% endblock %}

<script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
<script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js" integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM" crossorigin="anonymous"></script>
<script src="{{ siteUrl }}assets/forms/js/main.js"></script>

{% block footerJs %}{% endblock %}

</body>
</html>
